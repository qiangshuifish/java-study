<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mybatisplus.example.dao.UserMapper">
    <select id="selectByName" parameterType="java.lang.String" resultType="com.mybatisplus.example.entity.User">
        SELECT * from sys_user WHERE `username` = #{name}
    </select>
</mapper>
